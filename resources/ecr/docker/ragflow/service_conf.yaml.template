ragflow:
  host: ${RAGFLOW_HOST:-0.0.0.0}
  http_port: 9380
infinity:
  uri: '${INFINITY_HOST:-infinity}:23817'
  db_name: 'default_db'
redis:
  db: 1
  password: '${REDIS_PASSWORD:-infini_rag_flow}'
  host: '${REDIS_HOST:-redis}:6379'
postgres:
  name: '${POSTGRES_DBNAME:-rag_flow}'
  user: '${POSTGRES_USER:-rag_flow}'
  password: '${POSTGRES_PASSWORD:-infini_rag_flow}'
  host: '${POSTGRES_HOST:-postgres}'
  port: 5432
  max_connections: 100
  stale_timeout: 30
aws_s3:
  access_key: '${AWS_S3_ACCESS_KEY:-}'
  secret_key: '${AWS_S3_SECRET_KEY:-}'
  region: '${AWS_S3_REGION:-us-east-1}'
  endpoint_url: '${AWS_S3_ENDPOINT_URL:-}'
  bucket: '${AWS_S3_BUCKET:-}'
  prefix_path: '${AWS_S3_PREFIX_PATH:-}'
  signature_version: '${AWS_S3_SIGNATURE_VERSION:-v4}'
  addressing_style: '${AWS_S3_ADDRESSING_STYLE:-path}'
